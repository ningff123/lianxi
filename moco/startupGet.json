[
  {
    "description": "模拟一个没有参数的get请求",
    "request":{
      "uri": "/getdemo",
      "method": "get"
    },
    "response":{
      "text": "这是一个没有参数的get请求",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  },
  {
    "description": "模拟一个带参数的get请求",
    "request":{
      "uri":"/getWithParam",
      "method":"get",
      "queries":{
        "name":"zhangsan",
        "age":"100"
      }
    },
    "response":{
      "text":"zhangsan100岁了！！！",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  }
]